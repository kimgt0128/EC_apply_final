{{>components/header}}


<div class="container col-md h2 m-3 p-2 pt-5 text-primary text-center">
    <p class="fs-2 mt-2">지원 결과 확인</p>
</div>
<form class="container m-3 p-2" action="/forms/lookUp" method="post">
    <div class="container w-50 p-5 mb-3 border border-primary">
        <div class="col-md p-1">
            <label class="form-label">이름</label>
            <input type="text" class="form-control" name="name">
        </div>
        <div class="col-md p-1">
            <label class="form-label">전화 번호</label>
            <input type="text" class="form-control" name="phoneNumber" data-format="dddddddd">
        </div>
    </div>
    <div class="container mb-3 text-center">
        <button type="submit" class="btn btn-secondary">조회하기</button>
    </div>
</form>

{{>components/footer}}